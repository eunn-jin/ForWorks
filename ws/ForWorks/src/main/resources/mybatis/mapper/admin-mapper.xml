<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="adminMapper">

	<select id="selectOneById" resultType="com.kh.forworks.admin.vo.AdminVo">
		SELECT 
			MAN_NO
			, MAN_ID
			, MAN_PWD
			, MAN_MDATE
		FROM MANAGER
		WHERE MAN_ID = #{manId}
	</select>

</mapper>